{
  "name": "@kingteza/crud-component",
  "private": false,
  "version": "1.2.1",
  "description": "React CRUD component library with Ant Design",
  "keywords": [
    "react",
    "crud",
    "antd",
    "typescript",
    "types",
    "typings"
  ],
  "types": "./index.d.ts",
  "author": "Kingteza",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kingteza/crud-component"
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && vite build && cp package.json README.md .npmignore dist/",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "lint": "eslint .",
    "tsc": "tsc",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.es.js",
      "require": "./index.cjs.js"
    },
    "./common": {
      "types": "./common/index.d.ts",
      "import": "./common/index.es.js",
      "require": "./common/index.cjs.js"
    },
    "./util": {
      "types": "./util/index.d.ts",
      "import": "./util/index.es.js",
      "require": "./util/index.cjs.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@iconify/react": "^6.0.1",
    "bootstrap-css-only": "^4.4.1",
    "browser-image-compression": "^2.0.2",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "i18next": "^24.2.0",
    "mime": "^4.0.6",
    "papaparse": "^5.4.1",
    "path-browserify": "^1.0.1",
    "react-advanced-cropper": "^0.20.1",
    "react-highlight-words": "^0.20.0",
    "react-i18next": "^15.2.0",
    "react-quill": "^2.0.0",
    "react-responsive": "^10.0.0",
    "react-show-more-text": "^1.7.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@anolilab/semantic-release-pnpm": "^2.0.4",
    "@eslint/js": "^9.17.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.3.15",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-show-more-text": "^1.4.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "antd": "^5.22.7",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "semantic-release": "^25.0.1",
    "tsup": "^8.3.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "antd": ">=5.22.6",
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1",
    "react-router-dom": ">=6.3.0"
  }
}
