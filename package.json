{
  "name": "@kingteza/crud-component",
  "private": false,
  "version": "1.0.13",
  "description": "React CRUD component library with Ant Design",
  "keywords": [
    "react",
    "crud",
    "antd",
    "typescript",
    "types",
    "typings"
  ],
  "types": "./dist/index.d.ts",
  "author": "Kingteza",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kingteza/crud-component"
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts",
    "dist/components/",
    "dist/util/",
    "dist/locale/",
    "README.md"
  ],
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && vite build",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./common": {
      "types": "./dist/components/common/index.d.ts",
      "import": "./dist/components/common/index.js",
      "require": "./dist/components/common/index.cjs"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "import": "./dist/util/index.js",
      "require": "./dist/util/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "antd": "^5.22.6",
    "bootstrap-css-only": "^4.4.1",
    "browser-image-compression": "^2.0.2",
    "file-saver": "^2.0.5",
    "mime": "^4.0.6",
    "papaparse": "^5.4.1",
    "path-browserify": "^1.0.1",
    "react": "^18.3.1",
    "react-cropper": "^2.3.3",
    "react-dom": "^18.3.1",
    "react-highlight-words": "^0.20.0",
    "react-responsive": "^10.0.0",
    "react-router": "^7.1.1",
    "react-router-dom": "^7.1.1",
    "react-show-more-text": "^1.7.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.3.15",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-show-more-text": "^1.4.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "i18next": "^24.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^15.2.0",
    "tsup": "^8.3.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "i18next": ">=21.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-i18next": ">=11.0.0"
  }
}
